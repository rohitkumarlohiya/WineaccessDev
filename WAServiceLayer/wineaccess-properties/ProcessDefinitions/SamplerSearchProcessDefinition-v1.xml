<?xml version="1.0" encoding="UTF-8"?>
<orch:process-definition name="SamplerSearchProcessDefinition" version="v1" xmlns:orch="http://globallogic.com/orch">
	<start-state name="Start Task">
		<orch:transition name="SamplerTaskTransition" to="SamplerAdapter" />		
	</start-state>
	
	<task name="SamplerAdapter">					
		<action class="com.wineaccess.command.search.sampler.SamplerSearchAdapter"/>
		<orch:transition name="END" to="END-STATE"/>
	</task>	
	
	<end-state name="END-STATE"/>
</orch:process-definition>